{
  "name": "binary-inspectors",
  "version": "0.0.0",
  "description": "Web-based inspectors for binary formats.",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch --no-watchman",
    "format": "prettier --write src/**/*.ts src/**/*.tsx *.js",
    "check-format": "prettier-check src/**/*.ts src/**/*.tsx",
    "build": "rm -rf dist/ && webpack --env mode=production",
    "start": "webpack serve --open --env mode=development"
  },
  "author": "Varun Ramesh",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/jest": "^25.1.1",
    "chai": "^4.2.0",
    "query-string": "^6.10.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.2.1",
    "react-github-corner": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^17.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "fast-text-encoding": "^1.0.3",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "typescript": "^3.5.3",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  }
}
